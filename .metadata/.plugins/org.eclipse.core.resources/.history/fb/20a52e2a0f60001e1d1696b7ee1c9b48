package com.list.intro;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Node list=new Node(1);
		Node temp=list;
		for(int i=1;i<=10;i++) {
			Node list1=new Node(i);
			temp.next=list1;
			temp=temp.next;
		}
System.out.println(list);
	}

}


class Node{
	int data;
	Node next;
	public Node() {
		super();
		this.data = 0;
		this.next = null;
	}
	public Node(int data, Node next) {
		super();
		this.data = data;
		this.next = next;
	}
	public Node(int data) {
		super();
		this.data = data;
		this.next = null;
	}
	@Override
	public String toString() {
		return "Node [data=" + data + ", next=" + next + "]";
	}
	
	
}