package com.pattern.Number;

public class Main7_10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		p1();
		
		
	}
	public static void p1() {
//		5 4 3 2 1 
//		5 4 3 2 
//		5 4 3 
//		5 4 
//		5 
		
		for(int i=1;i<=5;i++) {
			for(int j=5;j>=i;j--) {
				System.out.print(j+" ");
			}
			System.out.println();
		}
	}

}
